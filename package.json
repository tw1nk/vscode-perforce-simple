{
	"name": "vscode-perforce-simple",
	"displayName": "vscode-perforce-simple",
	"description": "No nonsense perforce extension",
	"author": {"name": "Calle Gustafsson"},
	"publisher": "callegustafsson",
	"repository": {
		"url": "https://github.com/tw1nk/vscode-perforce-simple",
		"type": "git"
	},
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.p4edit",
		"*"
	],
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.p4edit",
				"title": "p4 edit"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
